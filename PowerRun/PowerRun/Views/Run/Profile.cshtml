@using PowerRun.ViewModels
@model RunEditModel
@{
    ViewData["Title"] = "Profile";
}
<h2>@ViewData["Title"]</h2>


<br />

<span>@User.Identity.Name</span>
<br />
<hr />

<h3> Averages </h3>

<table>
    <tr>
        <td>Average Distance</td>
        <td>@Model.Distance</td>
    </tr>
    <tr>
        <td>Average Time</td>
        <td>@Model.Time</td>
    </tr>
    <tr>
        <td>Average Pace</td>
        <td>@Model.Pace</td>
    </tr>
</table>

<br />


